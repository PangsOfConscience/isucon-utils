# 踏み台経由のconfig例

Host bastion
  HostName <REPLACE_YOUR_BASTION_IP>
  Port <REPLACE_YOUR_PORT>
  User isucon

Host server1
  ProxyJump isucon-bastion
  HostName <REPLACE_YOUR_SERVER1_IP>
  User isucon

Host server2
  ProxyJump isucon-bastion
  HostName <REPLACE_YOUR_SERVER2_IP>
  User isucon

Host server3
  ProxyJump isucon-bastion
  HostName <REPLACE_YOUR_SERVER2_IP>
  User isucon